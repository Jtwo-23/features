{
    "id": "ansible",
    "version": "2.0.0",
    "name": "Ansible (via pipx)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/ansible",
    "description": "Ansible is a suite of software tools that enables infrastructure as code.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version of Ansible (via pipx) to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/pipx-package:1.0.1"
    ],
    "dependencies": {
        "ghcr.io/devcontainers-contrib/features/pipx-package:1.0.1": {
            "package": "ansible-core",
            "injections": "ansible"
        }
    },
    "install_command": "echo 'Done!'",
    "test_command": "ansible --version"
}