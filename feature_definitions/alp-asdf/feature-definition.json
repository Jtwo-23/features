{
    "id": "alp-asdf",
    "version": "2.0.0",
    "name": "alp (via asdf)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/alp-asdf",
    "description": "Installs alp",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version of alp (via asdf) to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/asdf-package:1.0.0"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/asdf-package:1.0.0",
            "options": {
                "plugin": "alp",
                "version": "$options.version"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_command": "alp --version"
}