{
    "id": "lean-asdf",
    "version": "2.0.0",
    "name": "Lean (via asdf)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/lean-asdf",
    "description": "Lean is a functional programming language that makes it easy to write correct and maintainable code.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version of Lean (via asdf) to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/asdf-package:1.0.0"
    ],
    "dependencies": {
        "ghcr.io/devcontainers-contrib/features/asdf-package:1.0.0": {
            "plugin": "lean",
            "version": "$options.version"
        }
    },
    "install_command": "echo 'Done!'",
    "test_command": "lean --version"
}