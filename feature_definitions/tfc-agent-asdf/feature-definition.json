{
    "id": "tfc-agent-asdf",
    "version": "2.0.2",
    "name": "tfc-agent (via asdf)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/tfc-agent-asdf",
    "description": "Installs tfc-agent",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version of tfc-agent to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/asdf-package:1.0.2"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/asdf-package:1.0.2",
            "options": {
                "plugin": "tfc-agent",
                "version": "$options.version"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_command": "tfc-agent -v"
}