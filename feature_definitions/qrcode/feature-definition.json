{
    "id": "qrcode",
    "version": "2.0.0",
    "name": "qrcode (via pipx)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/qrcode",
    "description": "qrcode is a command line QR-Code generator.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version of qrcode (via pipx) to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/pipx-package:1.0.1"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/pipx-package:1.0.1",
            "options": {
                "package": "qrcode[pil]"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_command": "qr --version"
}