{
    "id": "express-generator",
    "version": "2.0.0",
    "name": "Express Application Generator (via npm)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/express-generator",
    "description": "Express Application Generator is a handy tool for creating skeleton Express web apps.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version of Express Application Generator (via npm) to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/npm-package:1.0.1"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/npm-package:1.0.1",
            "options": {
                "package": "express-generator",
                "version": "$options.version"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_command": "express --version"
}