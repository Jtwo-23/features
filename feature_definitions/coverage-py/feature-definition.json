{
    "id": "coverage-py",
    "version": "2.0.2",
    "name": "Coverage.py (via pipx)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/coverage-py",
    "description": "Coverage.py is a tool for measuring code coverage of Python programs.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version of Coverage.py to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/pipx-package:1.1.1",
        "ghcr.io/devcontainers/features/python:1.0.18"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/pipx-package:1.1.1",
            "options": {
                "package": "coverage"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_command": "coverage --version"
}