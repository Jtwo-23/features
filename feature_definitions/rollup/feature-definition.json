{
    "id": "rollup",
    "version": "2.0.0",
    "name": "rollup.js (via npm)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/rollup",
    "description": "Rollup is a module bundler for JavaScript which compiles small pieces of code into something larger and more complex, such as a library or application.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version of rollup.js (via npm) to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/npm-package:1.0.1"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/npm-package:1.0.1",
            "options": {
                "package": "rollup",
                "version": "$options.version"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_command": "rollup --version"
}