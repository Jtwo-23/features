{
    "id": "elasticsearch-asdf",
    "version": "2.0.0",
    "name": "Elasticsearch (via asdf)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/elasticsearch-asdf",
    "description": "Elasticsearch is a search engine based on the Lucene library.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version of Elasticsearch (via asdf) to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/asdf-package:1.0.0"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/asdf-package:1.0.0",
            "options": {
                "plugin": "elasticsearch",
                "version": "$options.version"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_command": "elasticsearch --version"
}