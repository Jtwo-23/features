{
    "id": "cookiecutter",
    "version": "2.0.4",
    "name": "Cookiecutter (via pipx)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/cookiecutter",
    "description": "Cookiecutter creates projects from project templates.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version of Cookiecutter to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/pipx-package:1.1.1",
        "ghcr.io/devcontainers/features/python:1.0.18"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/pipx-package:1.1.1",
            "options": {
                "package": "cookiecutter"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_command": "cookiecutter --version"
}