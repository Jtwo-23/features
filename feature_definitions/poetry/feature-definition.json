{
    "id": "poetry",
    "version": "2.0.0",
    "name": "Poetry (via pipx)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/poetry",
    "description": "Poetry is a tool for dependency management and packaging in Python.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version of Poetry (via pipx) to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/pipx-package:1.0.1"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/pipx-package:1.0.1",
            "options": {
                "package": "poetry"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_command": "poetry --version"
}