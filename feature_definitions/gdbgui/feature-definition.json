{
    "id": "gdbgui",
    "version": "2.0.3",
    "name": "gdbgui (via pipx)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/gdbgui",
    "description": "gdbgui is a browser-based frontend to gdb (gnu debugger).",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version of gdbgui to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/pipx-package:1.1.1",
        "ghcr.io/devcontainers/features/python:1.0.18"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/pipx-package:1.1.1",
            "options": {
                "package": "gdbgui"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_command": "gdbgui --version"
}