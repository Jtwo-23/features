{
    "id": "gdbgui",
    "version": "2.0.0",
    "name": "gdbgui (via pipx)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/gdbgui",
    "description": "gdbgui is a browser-based frontend to gdb (gnu debugger).",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version of gdbgui (via pipx) to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/pipx-package:1.0.1"
    ],
    "dependencies": {
        "ghcr.io/devcontainers-contrib/features/pipx-package:1.0.1": {
            "package": "gdbgui"
        }
    },
    "install_command": "echo 'Done!'",
    "test_command": "gdbgui --version"
}