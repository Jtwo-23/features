{
    "id": "black",
    "version": "2.0.0",
    "name": "Black (via pipx)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/black",
    "description": "Black is an uncompromising Python code formatter.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version of Black (via pipx) to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/pipx-package:1.0.1"
    ],
    "dependencies": {
        "ghcr.io/devcontainers-contrib/features/pipx-package:1.0.1": {
            "package": "black"
        }
    },
    "install_command": "echo 'Done!'",
    "test_command": "black --version"
}