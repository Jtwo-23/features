{
    "name": "Bikeshed (via pipx)",
    "id": "bikeshed",
    "version": "1.0.1",
    "description": "A preprocessor for anyone writing specifications that converts source files into actual specs.",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/bikeshed",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest"
            ],
            "default": "latest",
            "description": "Select the version of Bikeshed to install."
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/pipx-package:1.0.1"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/pipx-package:1.0.1",
            "options": {
                "package": "bikeshed"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_command": "bandit --version"
}

