{
    "id": "boundary-asdf",
    "version": "2.0.0",
    "name": "boundary (via asdf)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/boundary-asdf",
    "description": "Installs boundary",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version of boundary (via asdf) to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/asdf-package:1.0.0"
    ],
    "dependencies": {
        "ghcr.io/devcontainers-contrib/features/asdf-package:1.0.0": {
            "plugin": "boundary",
            "version": "$options.version"
        }
    },
    "install_command": "echo 'Done!'",
    "test_command": "boundary version"
}