{
    "id": "flit",
    "version": "2.0.0",
    "name": "Flit (via pipx)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/flit",
    "description": "Flit is a simple way to put Python packages and modules on PyPI.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version of Flit (via pipx) to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/pipx-package:1.0.1"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/pipx-package:1.0.1",
            "options": {
                "package": "flit"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_command": "flit --version"
}