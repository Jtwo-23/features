{
    "id": "brownie",
    "version": "2.0.0",
    "name": "Brownie (via pipx)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/brownie",
    "description": "Brownie is a Python-based development and testing framework for smart contracts targeting the Ethereum Virtual Machine.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version of Brownie (via pipx) to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/pipx-package:1.0.1"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/pipx-package:1.0.1",
            "options": {
                "package": "eth-brownie"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_command": "brownie --version"
}