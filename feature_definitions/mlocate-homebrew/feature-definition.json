{
    "id": "mlocate-homebrew",
    "version": "1.0.0",
    "name": "mlocate (via Homebrew)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/mlocate-homebrew",
    "description": "mlocate is a locate/updatedb implementation used for quick lookup of file names.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version of mlocate to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/homebrew-package:1.0.0"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/homebrew-package:1.0.0",
            "options": {
                "package": "mlocate",
                "version": "$options.version"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_command": "locate --version"
}