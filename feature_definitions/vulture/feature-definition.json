{
    "id": "vulture",
    "version": "2.0.0",
    "name": "Vulture (via pipx)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/vulture",
    "description": "Vulture finds unused code in Python programs.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version of Vulture (via pipx) to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/pipx-package:1.0.1"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/pipx-package:1.0.1",
            "options": {
                "package": "vulture"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_command": "vulture --version"
}