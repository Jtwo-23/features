{
    "id": "peco-asdf",
    "version": "2.0.0",
    "name": "peco (via asdf)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/peco-asdf",
    "description": "peco is simplistic interactive filtering tool.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version of peco (via asdf) to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/asdf-package:1.0.0"
    ],
    "dependencies": {
        "ghcr.io/devcontainers-contrib/features/asdf-package:1.0.0": {
            "plugin": "peco",
            "version": "$options.version"
        }
    },
    "install_command": "echo 'Done!'",
    "test_command": "peco --version"
}