{
    "id": "svu-asdf",
    "version": "2.0.3",
    "name": "svu (via asdf)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/svu-asdf",
    "description": "svu is a tool to manage semantic versions at ease.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version of svu to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/asdf-package:1.0.2"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/asdf-package:1.0.2",
            "options": {
                "plugin": "svu",
                "version": "$options.version"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_command": "svu --version"
}