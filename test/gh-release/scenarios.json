{
    "test_defaults_debian": {
        "image": "mcr.microsoft.com/devcontainers/base:debian",
        "features": {
            "gh-release": {
                "version": "latest",
                "repo": "cli/cli",
                "target": "gh"
            }
        }
    },
    "test_defaults_alpine": {
        "image": "mcr.microsoft.com/devcontainers/base:alpine",
        "features": {
            "gh-release": {
                "version": "latest",
                "repo": "cli/cli",
                "target": "gh"
            }
        }
    },
    "test_act": {
        "image": "mcr.microsoft.com/devcontainers/base:debian",
        "features": {
            "gh-release": {
                "version": "latest",
                "repo": "nektos/act",
                "target": "act"
            }
        }
    },
    "test_powershell": {
        "image": "mcr.microsoft.com/devcontainers/base:debian",
        "features": {
            "gh-release": {
                "version": "latest",
                "repo": "PowerShell/PowerShell",
                "target": "pwsh",
                "assetRegex": "^(?!.*(fxdependent))"
            }
        }
    }
}